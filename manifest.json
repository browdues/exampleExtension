{
  "name": "Getting Started Example",
  "description": "Build a Chrome Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "dist/background.js"
  },
  "permissions": ["storage", "scripting", "tabs", "activeTab"],
  "action": {
    "default_popup": "/views/popup.html",
    "default_icon": "/assets/16.png"
  }
}
